{
  "name": "quyen-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --config nodemon.json src/app.ts",
    "start": "node --experimental-specifier-resolution=node .build/app.js",
    "build": "npx tsc -p .",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate",
    "migration": "npx sequelize-cli migration:generate --name",
    "seed": "npx sequelize-cli seed:generate --name",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:name": "npx sequelize-cli db:seed:undo --seed"
  },
  "imports": {
    "@src/*": "./src/*"
  },
  "author": "quyen",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.4",
    "sequelize-typescript": "^2.1.3",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.184",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.8",
    "@types/sequelize": "^4.28.14",
    "@types/validator": "^13.7.5",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0"
  }
}
